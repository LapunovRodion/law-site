{
  "kind": "collectionType",
  "collectionName": "study_groups",
  "info": {
    "singularName": "study-group",
    "pluralName": "study-groups",
    "displayName": "StudyGroup "
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "name",
      "required": true
    },
    "groupNumber": {
      "type": "string",
      "required": true
    },
    "Course": {
      "type": "integer",
      "required": true
    },
    "educationLevel": {
      "type": "enumeration",
      "required": true,
      "enum": [
        "bachelor",
        "master",
        "postgraduate",
        "other"
      ]
    },
    "course_loads": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::course-load.course-load",
      "mappedBy": "study_group"
    },
    "lessons": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::lesson.lesson",
      "mappedBy": "study_group"
    },
    "lesson_overrides": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::lesson-override.lesson-override",
      "mappedBy": "study_group"
    }
  }
}
